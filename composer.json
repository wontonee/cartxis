{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/vue-starter-kit",
    "type": "project",
    "description": "Vortex Ecommerce Open Source For Laravel Inertia JS VUE.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "aws/aws-sdk-php": "^3.357",
        "inertiajs/inertia-laravel": "^2.0",
        "intervention/image": "^3.0",
        "laravel/fortify": "^1.30",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.10.1",
        "laravel/wayfinder": "^0.1.9",
        "mews/purifier": "^3.4",
        "mpdf/mpdf": "^8.2",
        "razorpay/razorpay": "^2.9",
        "stripe/stripe-php": "^18.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.18",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^4.1",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Vortex\\Core\\": "packages/Vortex/Core/",
            "Vortex\\Core\\Database\\Seeders\\": "packages/Vortex/Core/Database/Seeders/",
            "Vortex\\Admin\\": "packages/Vortex/Admin/",
            "Vortex\\Admin\\Database\\Seeders\\": "packages/Vortex/Admin/Database/Seeders/",
            "Vortex\\Product\\": "packages/Vortex/Product/",
            "Vortex\\Product\\Database\\Seeders\\": "packages/Vortex/Product/Database/Seeders/",
            "Vortex\\Cart\\": "packages/Vortex/Cart/",
            "Vortex\\Shop\\": "packages/Vortex/Shop/",
            "Vortex\\Shop\\Database\\Seeders\\": "packages/Vortex/Shop/Database/Seeders/",
            "Vortex\\Settings\\": "packages/Vortex/Settings/",
            "Vortex\\Settings\\Database\\Seeders\\": "packages/Vortex/Settings/Database/Seeders/",
            "Vortex\\Stripe\\": "packages/Vortex/Stripe/src/",
            "Vortex\\Razorpay\\": "packages/Vortex/Razorpay/src/",
            "Vortex\\Sales\\": "packages/Vortex/Sales/src/",
            "Vortex\\Customer\\": "packages/Vortex/Customer/src/",
            "Vortex\\CMS\\": "packages/Vortex/CMS/src/",
            "Vortex\\System\\": "packages/Vortex/System/src/",
            "Vortex\\Reports\\": "packages/Vortex/Reports/src/",
            "Vortex\\Marketing\\": "packages/Vortex/Marketing/src/",
            "Vortex\\API\\": "packages/Vortex/API/",
            "Vortex\\Setup\\": "packages/Vortex/Setup/src/",
            "Vortex\\Setup\\Database\\Seeders\\": "packages/Vortex/Setup/src/Database/Seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "setup": [
            "composer install",
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate",
            "@php artisan migrate --force",
            "npm install",
            "npm run build"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"npm run dev\" --names='server,queue,vite'"
        ],
        "dev:ssr": [
            "npm run build:ssr",
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"php artisan inertia:start-ssr\" --names=server,queue,logs,ssr --kill-others"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "pre-package-uninstall": [
            "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
