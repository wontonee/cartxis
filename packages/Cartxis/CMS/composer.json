{
    "name": "cartxis/cms",
    "description": "Content Management System for Cartxis Commerce",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Cartxis Team",
            "email": "dev@cartxis.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "mews/purifier": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "Cartxis\\CMS\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cartxis\\CMS\\Providers\\CMSServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
